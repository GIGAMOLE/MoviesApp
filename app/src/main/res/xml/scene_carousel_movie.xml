    <Transition
        android:id="@+id/transition_carousel_movie_forward"
        motion:constraintSetEnd="@+id/constraint_set_carousel_movie_next"
        motion:constraintSetStart="@+id/constraint_set_carousel_movie_start"
        app:motionInterpolator="easeIn">
        <OnSwipe
            motion:dragDirection="dragRight"
            motion:onTouchUp="autoComplete" />
    </Transition>
    <Transition
        android:id="@+id/transition_carousel_movie_backward"
        motion:constraintSetEnd="@+id/constraint_set_carousel_movie_previous"
        motion:constraintSetStart="@+id/constraint_set_carousel_movie_start"
        app:motionInterpolator="easeIn">
        <OnSwipe
            motion:dragDirection="dragLeft"
            motion:onTouchUp="autoComplete" />
    </Transition>
    <Transition
        android:id="@+id/transition_carousel_movie_open"
        app:constraintSetStart="@+id/constraint_set_carousel_movie_start"
        app:motionInterpolator="cubic(0.06,0.3,0.81,1)"
        motion:constraintSetEnd="@+id/constraint_set_carousel_movie_open"
        motion:duration="800"
        app:pathMotionArc="startHorizontal">
        <KeyFrameSet>
            <KeyAttribute
                app:framePosition="1"
                app:motionTarget="@+id/movie_widget_current">
                <CustomAttribute
                    app:attributeName="movieImagePanY"
                    app:customFloatValue="-0.8" />
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="99"
                app:motionTarget="@+id/movie_widget_current">
                <CustomAttribute
                    app:attributeName="movieImagePanY"
                    app:customFloatValue="-0.8" />
            </KeyAttribute>
        </KeyFrameSet>
    </Transition>
